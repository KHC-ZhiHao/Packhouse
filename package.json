{
  "name": "packhouse",
  "version": "3.0.0",
  "description": "Packhouse is a function wrapper based on the concept of functional programming",
  "main": "dist/index.js",
  "types": "types/main.d.ts",
  "scripts": {
    "test": "nyc mocha --require ts-node/register test/*.ts",
    "prod": "npm run test && cross-env webpack --progress --hide-modules",
    "build": "cross-env webpack --progress --hide-modules",
    "coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KHC-ZhiHao/Packhouse.git"
  },
  "keywords": [
    "lambda",
    "azure functions",
    "serverless",
    "functional programming"
  ],
  "author": "KHC-ZhiHao",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/KHC-ZhiHao/Packhouse/issues"
  },
  "homepage": "https://github.com/KHC-ZhiHao/Packhouse#readme",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "exclude": [
      "**/*.js",
      "**/*.d.ts",
      "node_modules",
      "test/**/*.ts"
    ],
    "all": true
  },
  "devDependencies": {
    "@types/chai": "^4.2.12",
    "@types/mocha": "^8.0.0",
    "@types/node": "^14.0.26",
    "chai": "^4.2.0",
    "coveralls": "^3.1.0",
    "cross-env": "^7.0.2",
    "mocha": "^8.0.1",
    "nyc": "^15.1.0",
    "ts-loader": "^8.0.1",
    "ts-node": "^8.10.2",
    "tslint": "^6.1.2",
    "tslint-config-standard": "^9.0.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.9.7",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12"
  }
}
